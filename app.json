{
  "expo": {
    "scheme": "tourinapp",
    "name": "tourin-app",
    "slug": "tourin-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/dolmen-icon.jpeg",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/dolmen-icon.jpeg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Mostramos tu ubicaci√≥n en el mapa."
      }
    },
    "android": {
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/dolmen-icon.jpeg",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.dolmen.tourinapp"
    },
    "web": {
      "favicon": "./assets/dolmen-icon.jpeg"
    },
    "plugins": [
      "expo-router",
      ["@maplibre/maplibre-react-native"]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d718b8a6-38af-4b2d-b470-1d5b4e4cb5aa"
      }
    },
    "owner": "jorgemedinaleigh"
  }
}
